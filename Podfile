  pod 'Swinject', '~> 2.6.0'
  pod 'SwinjectStoryboard', '~> 2.2.0'
  pod 'RealmSwift', '~> 3.14.2'

  target 'fosdemTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'fosdemUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
